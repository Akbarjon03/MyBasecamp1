<p class="fw-bolder fs-1 text-center">Projects</p>
  <br>
  <div id="projects">
    <% @projects.each do |project| %>
    
    <% if project.user.id == current_user.id %>
      <%= render project %>
      <p>
        <%= link_to "Show this project", project, class:"show"%>
      </p>
    <% end %>
    <% end %>
  </div>
  <%= link_to "New project", new_project_path, class:"new" %>
  <%= button_to "Log Out", destroy_user_session_path, method: :delete , class: "logout"%>
